{
  "name": "winas",
  "version": "0.3.5",
  "description": "WinNAS",
  "author": "WISNUC <support@winsuntech.cn>",
  "repository": "https://github.com/wisnuc/winas-desktop.git",
  "private": true,
  "main": "main.js",
  "scripts": {
    "i18n": "node test/checkI18n.js",
    "dist-x64": "./node_modules/.bin/build --config ./electron-builder.yml --win --x64 -p never",
    "dist-ia32": "./node_modules/.bin/build --config ./electron-builder.yml --win --ia32 -p never",
    "dist-mac": "./node_modules/.bin/build --config ./electron-builder.yml --mac -p never",
    "eslint": "eslint --fix lib/*.js src/*/*.jsx",
    "cloud": "NODE_ENV=cloud electron .",
    "mocha": "mocha --compilers js:babel-core/register --recursive --sort",
    "rebuild": "electron-rebuild",
    "start": "electron .",
    "webpack": "rm -f public/*.js && rm -f public/*.json && webpack --watch",
    "webpack2": "webpack -p --devtool eval",
    "analyzer": "webpack -p --devtool eval --profile --json > public/stat.json && webpack-bundle-analyzer public/stat.json"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.11",
    "electron": "^4.0.0-beta.7",
    "electron-builder": "^20.25.0",
    "electron-rebuild": "^1.7.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-jsx": "^5.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "json-loader": "^0.5.7",
    "mocha": "^5.0.5",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.20.3",
    "webpack": "^2.7.0",
    "webpack-bundle-analyzer": "^3.0.2"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "bluebird": "^3.5.1",
    "debug": "^3.1.0",
    "eventemitter3": "^3.0.1",
    "gsap": "^1.20.4",
    "i18n": "^0.8.3",
    "keycode": "^2.2.0",
    "material-ui": "^0.20.0",
    "md5": "^2.2.1",
    "mdns-js": "^1.0.1",
    "mkdirp": "^0.5.1",
    "node-machine-id": "^1.1.10",
    "nodejs-fs-utils": "^1.1.0",
    "pdfjs-dist": "2.0.267",
    "prettysize": "^1.1.0",
    "react": "^15.6.1",
    "react-event-listener": "^0.5.3",
    "react-json-tree": "^0.11.0",
    "react-pdf-pages": "^0.1.8",
    "react-transition-group": "^1.2.1",
    "react-virtualized": "^9.18.5",
    "redux": "^3.7.2",
    "rimraf": "^2.6.2",
    "sanitize-filename": "^1.6.1",
    "superagent": "^3.8.2",
    "uuid": "^3.2.1",
    "validator": "^9.4.1"
  },
  "optionalDependencies": {
    "fs-xattr": "^0.2.1",
    "fs-ads": "^1.1.0"
  },
  "src": "src",
  "test": "test",
  "public": "public"
}
